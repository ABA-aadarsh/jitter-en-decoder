{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src":"/auth",
        "dest":"index.js"
      },
      {
        "src":"/auth/(.*)",
        "dest":"index.js"
      },
      {
        "src":"/api",
        "dest":"index.js"
      },
      {
        "src":"/api/(.*)",
        "dest":"index.js"
      },
      {
        "src":"/",
        "dest":"dist/index.html"
      },
      {
        "src":"/encode",
        "dest":"dist/index.html/encode"
      },
      {
        "src":"/decode",
        "dest":"dist/index.html/decode"
      },
      {
        "src":"/login",
        "dest":"dist/index.html/login"
      },
      {
        "src":"/signup",
        "dest":"dist/index.html/signup"
      }
    ]
  }